{
  "include": [
    "src"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.venv",
    "**/node_modules",
    "**/.pytest_cache",
    "**/worktrees",
    ".claude.backup.*",
    "dotnet",
    "spa",
    "tests",
    "demos",
    "examples",
    "visualizations",
    ".venv"
  ],
  "ignore": [
    "**/node_modules",
    "**/__pycache__",
    ".venv"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.12",
  "pythonPlatform": "Linux",
  "typeCheckingMode": "basic"
}
