{
  "terraform": {
    "required_providers": {
      "azurerm": {
        "source": "hashicorp/azurerm",
        "version": ">=3.0"
      },
      "random": {
        "source": "hashicorp/random",
        "version": ">=3.1"
      },
      "tls": {
        "source": "hashicorp/tls",
        "version": ">=4.0"
      }
    }
  },
  "provider": {
    "azurerm": {
      "features": {},
      "resource_provider_registrations": "none",
      "subscription_id": "${var.subscription_id}"
    }
  },
  "variable": {
    "subscription_id": {
      "description": "Azure subscription ID for deployment",
      "type": "string",
      "default": ""
    }
  },
  "resource": {}
}